  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@tscircuit/file-server": "^0.0.11",
    "@tscircuit/runframe": "^0.0.47",
    "chokidar": "^4.0.1",
    "commander": "^12.1.0",
    "configstore": "^7.0.0",
    "cosmiconfig": "^9.0.0",
    "delay": "^6.0.0",
    "ky": "^1.7.4",
    "perfect-cli": "^1.0.20"
